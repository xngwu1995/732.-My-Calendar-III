class Solution:
    def merge(self, intervals: List[List[int]]) -> List[List[int]]:
        intervals = sorted(intervals, key = lambda x:x[0])
        ans = []
        for i in intervals:
            if ans and i[0] <= ans[-1][1]:
                ans[-1][1] = max(ans[-1][1],i[1])
            else:
                ans.append(i)
        return ans
